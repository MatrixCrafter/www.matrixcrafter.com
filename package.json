    "gatsby-transformer-remark": "^6.12.0",
    "gatsby-transformer-sharp": "^5.12.0",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.23",
    "styled-components": "^6.0.7",
    "styled-reset": "^4.5.1",
