    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-typescript": "^5.10.0",
    "graphql": "^16.6.0",
    "husky": "^8.0.3",
